utValue = '';\r\n    }\r\n\r\n    onDeleteEquipment(id) {\r\n        this.editSettings.removeEquipment(id);\r\n    }\r\n\r\n    cancelChanges() {\r\n        this.props.appStore.removeDialogComponent();\r\n        this.editSettings = null;\r\n    }\r\n\r\n    completeChanges() {\r\n        this.props.appStore.saveSettings(this.editSettings);\r\n\r\n        this.cancelChanges();\r\n    }\r\n\r\n    render() {\r\n        const { appStore } = this.props;\r\n\r\n        return <>\r\n            {this.editSettings ? <DialogContent>\r\n                <Container>\r\n                    <DialogTitle>Загальні налаштування</DialogTitle>\r\n                </Container>\r\n\r\n                <Container>\r\n                    <FormControlLabel\r\n                        control={<Switch \r\n                            onClick={this.changeWarState}\r\n                            checked={this.editSettings.isWarState}\r\n                        />}\r\n                        label={'Військовий стан'}\r\n                    />\r\n                </Container>\r\n                \r\n\r\n                {this.editSettings.equipmentsBase.length > 0 && <>\r\n                    <Divider/>\r\n                    \r\n                    <Container>\r\n                        <DialogTitle>Все доступне обладнання</DialogTitle>\r\n                    </Container>\r\n                    <List style={{ maxHeight: '200px', height: '200px', overflow: 'auto' }}>\r\n                        {this.editSettings.equipmentsBase.map(unit => <>\r\n                            <ListItem key={unit.id}>\r\n                                <ListItemText>{unit.name}</ListItemText>\r\n                                <ListItemIcon \r\n                                    onClick={() => this.onDeleteEquipment(unit.id)}\r\n                                ><Button color={'secondary'}><Cancel/></Button></ListItemIcon>\r\n                            </ListItem>\r\n                        </>)}\r\n                    </List>\r\n                </>}\r\n\r\n                <Divider/>\r\n\r\n                <Container>\r\n                    <DialogTitle>Додати нове обладнання</DialogTitle>\r\n                </Container>\r\n\r\n                <Container style={{ marginBottom: '10px' }}>\r\n                    <Input \r\n                        value={this.inputValue}\r\n                        onChange={this.onChangeInput}\r\n                        placeholder={'Введіть назву'}\r\n                    />\r\n                    <Button \r\n                        color={'primary'}\r\n                        onClick={this.onAddEquipment}\r\n                    ><Add/></Button>\r\n                </Container>\r\n\r\n                <Divider/>\r\n\r\n                <ButtonGroup variant={'text'} fullWidth>\r\n                    <Button \r\n                        color={'secondary'}\r\n                        onClick={this.cancelChanges}\r\n                    >Відмінити</Button>\r\n                    <Button \r\n                        color={'primary'}\r\n                        onClick={this.completeChanges}\r\n                    >Підтвердити</Button>\r\n                </ButtonGroup>\r\n            </DialogContent> : <CircularProgress/>}\r\n        </>\r\n    }\r\n\r\n}\r\n\r\nexport default inject('appStore')(observer(SettingsComponent));","C:\\Users\\VLAD\\Desktop\\WebstormProjects\\diplom\\front\\src\\components\\CreateChangeMapObject.jsx",["58","59"],"C:\\Users\\VLAD\\Desktop\\WebstormProjects\\diplom\\front\\src\\stores\\contracts\\MilEquipment.js",[],"C:\\Users\\VLAD\\Desktop\\WebstormProjects\\diplom\\front\\src\\stores\\contracts\\MilBase.js",[],"C:\\Users\\VLAD\\Desktop\\WebstormProjects\\diplom\\front\\src\\stores\\contracts\\MilWarehouse.js",[],"C:\\Users\\VLAD\\Desktop\\WebstormProjects\\diplom\\front\\src\\components\\ObjectInfoWindow.jsx",["60","61","62"],{"ruleId":"63","severity":1,"message":"64","line":3,"column":27,"nodeType":"65","messageId":"66","endLine":3,